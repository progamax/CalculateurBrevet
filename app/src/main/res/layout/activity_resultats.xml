<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.maxenceb.calculateurbrevet.Resultats"
    android:nestedScrollingEnabled="false"
    android:transitionGroup="false">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignTop="@+id/scrollView"
            android:layout_centerHorizontal="true">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Vos moyennes"
                android:id="@+id/textView9"
                android:layout_centerHorizontal="true"
                android:layout_alignParentTop="true" />

            <Space
                android:layout_width="wrap_content"
                android:layout_height="10dp"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="false"
                android:layout_below="@+id/textView9"
                android:layout_alignParentLeft="true"
                android:id="@+id/space"
                android:layout_alignParentStart="false"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Vos points : "
                android:id="@+id/textView13"
                android:layout_below="@id/space"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Medium Text"
                android:id="@+id/textView14"
                android:layout_toRightOf="@+id/textView13"
                android:layout_below="@+id/space" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/textView17"
                android:layout_below="@+id/textView13"
                android:nestedScrollingEnabled="false"
                android:singleLine="false"
                android:elegantTextHeight="false"
                android:autoText="false"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:editable="false"
                android:outlineProvider="bounds"
                android:text="Medim" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="Mentions"
                android:id="@+id/textView15"
                android:layout_below="@+id/textView17" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Il vous faut à chaque épreuve pour la mention :"
                android:id="@+id/textView16"
                android:layout_below="@+id/textView15" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="- Assez bien : "
                android:id="@+id/textView18"
                android:layout_below="@+id/textView16" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Medium Text"
                android:id="@+id/textView19"
                android:layout_below="@+id/textView16"
                android:layout_toRightOf="@+id/textView18" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text=" sur 20"
                android:id="@+id/textView20"
                android:layout_toRightOf="@+id/textView19"
                android:layout_below="@+id/textView16" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="- Bien : "
                android:id="@+id/textView21"
                android:layout_below="@+id/textView19" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Medium Text"
                android:id="@+id/textView22"
                android:layout_below="@+id/textView19"
                android:layout_toRightOf="@+id/textView21" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text=" sur 20"
                android:id="@+id/textView23"
                android:layout_toRightOf="@+id/textView22"
                android:layout_below="@+id/textView19" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="- Très bien : "
                android:id="@+id/textView24"
                android:layout_below="@+id/textView22" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Medium Text"
                android:id="@+id/textView25"
                android:layout_below="@+id/textView22"
                android:layout_toRightOf="@+id/textView24" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text=" sur 20"
                android:id="@+id/textView26"
                android:layout_toRightOf="@+id/textView25"
                android:layout_below="@+id/textView22" />

        </RelativeLayout>
    </ScrollView>

</RelativeLayout>
